        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i36" />

    <CheckBox
        android:id="@+id/i37"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:text="37. Grapas, soportes de ballestas, ojales, amortiguadores, bolsas de aire, barra Panhard: reapretar fijación."
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/i38"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <CheckBox
        android:id="@+id/i38"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:text="38. Bolsones de aire: limpiar y verificar estado."
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/i39"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i37" />

    <CheckBox
        android:id="@+id/i39"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:text="39. Válvula niveladora: verificar estado del vástago, verificar altura de la suspensión y regular si necesario."
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/btnSiguiente6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i38" />

    <Button
        android:id="@+id/btnSiguiente6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="293dp"
        android:layout_marginLeft="293dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Siguiente"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i39" />

</androidx.constraintlayout.widget.ConstraintLayout>